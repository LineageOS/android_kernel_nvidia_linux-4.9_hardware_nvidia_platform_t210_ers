#include "tegra210.dtsi"
#include <dt-bindings/interrupt-controller/arm-gic.h>
#include "tegra210-platforms/tegra210-ers-dfll-e2174-1101-a00.dtsi"
/ {

	serial@70006000 {
		compatible = "nvidia,tegra210-uart";
		status = "okay";
	};

	serial@70006040 {
		compatible = "nvidia,tegra210-uart";
		status = "okay";
	};

	udc@7d000000 {
		nvidia,port-otg;
		nvidia,charging-supported;
		nvidia,enable-pmu-vbus-detection;
		nvidia,id-detection-type = <2>;
		extcon-cables = <&bq2419x 0>;
		extcon-cable-names = "vbus";
		#extcon-cells = <1>;
		status = "okay";
	};

	otg@7d000000 {
		nvidia,enable-pmu-vbus-detection;
		nvidia,id-detection-type = <2>;
		nvidia,id-detection-gpio = <&max77620 0 0>;
		extcon-cables = <&bq2419x 0>;
		extcon-cable-names = "vbus";
		#extcon-cells = <1>;
		status = "okay";
	};
};
